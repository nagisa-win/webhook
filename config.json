{
    "server": {
        "port": 8080,
        "host": "0.0.0.0"
    },
    "routes": {
        "listen and response": {
            "path": "/",
            "method": ["GET", "POST"],
            "handler": "listen.js"
        },
        "health check": {
            "path": "/health",
            "method": ["GET"],
            "handler": "health.js"
        },
        "doc hook": {
            "path": "/doc",
            "method": ["GET", "POST"],
            "handler": "doc-hook.js"
        }
    }
}
