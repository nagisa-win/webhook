{
    "name": "webhook-server",
    "version": "1.0.0",
    "description": "Dynamic webhook server with configurable routes and handlers",
    "main": "app.js",
    "type": "module",
    "scripts": {
        "start": "node app.js",
        "dev": "nodemon app.js",
        "pm2:start": "pm2 start pm2.config.cjs",
        "pm2:stop": "pm2 stop pm2.config.cjs",
        "pm2:restart": "pm2 restart pm2.config.cjs",
        "pm2:logs": "pm2 logs webhook-server",
        "pm2:status": "pm2 status",
        "lint": "prettier --write \"**/*.{js,jsx,md,json}\""
    },
    "keywords": [
        "webhook",
        "express",
        "pm2",
        "dynamic-routing"
    ],
    "author": "nagisa-win",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0"
    },
    "devDependencies": {
        "nodemon": "^3.0.2",
        "prettier": "^3.6.2"
    }
}
